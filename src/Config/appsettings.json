{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "C:\\Logs\\compliments_complaints_service\\log-.log",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [
      "FromLogContext"
    ]
  },
  "ElasticSearchAwsConfiguration": {
    "Region": "eu-west-1",
    "IndexFormat": "compliments_complaints_service-local-{0:yyyy.MM-dd}",
    "InlineFields": false,
    "MinimumLevel": "Debug",
    "Enabled": false,
    "Url": "https://search-stockport-logs-4evarozpqvlofjsnxzliq7eqdy.eu-west-1.es.amazonaws.com/"
  },
  "Availability": {
    "BaseUrl": "http://scnavaildev.stockport.gov.uk/api/v1",
    "ErrorRoute": "/error/500",
    "WhitelistedRoutes": [
      "/swagger/index.html"
    ],
    "Environment": "local",
    "AllowSwagger": true
  },
  "BaseHttpClientConfiguration": {
    "Timeout": 40
  },
  "HttpClientConfiguration": [
    {
      "name": "verintServiceGateway",
      "baseUrl": "http://scnwebdev1.stockport.gov.uk:8050/"
    }
  ]
}
